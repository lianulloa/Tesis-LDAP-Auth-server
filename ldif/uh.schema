objectclass ( 1.3.6.1.4.1.53027.1.4.1 NAME 'Todos'
	DESC 'Datos comunes a todas las personas de la Universidad de la Habana'
	SUP person STRUCTURAL
	MAY ( Correo $ CI $ Edad $ Sexo $ Raza $ Direccion $ Municipio $ Provincia $ Pais $ LugarDeNacimiento $ Ciudadania $ TelefonoPersonal $ UJC $ PCC $ EsBaja $ FechaDeBaja $ FechaDeRegistro $ FechaDeCreacion $ TieneCorreo $ TieneInternet $ TieneChat $ CuotaInternet))

objectclass ( 1.3.6.1.4.1.53027.1.4.2 NAME 'Estudiante'
	DESC 'Estudiante de la Universidad de la Habana'
	SUP Todos STRUCTURAL
	MUST (uid) 
	MAY (IdSigenu $ IdFacultad $ Facultad $ Carrera $ TipoDeCurso $ Grade $ Grupo $ ResidenciaEstudiantil $ Becado))


objectclass ( 1.3.6.1.4.1.53027.1.4.3 NAME 'Trabajador'
	DESC 'Trabajador de la Universidad de la Habana'
	SUP Todos STRUCTURAL
	MUST (uid) 
	MAY (Assets $ IdAssets $ CodigoDeDependencia $ Dependencia $ CodigoDelArea $ Area $ Cargo $ CategoriaDocenteInvestigativa $ GradoCientifico $ DireccionDelCentro $ TelefonoDelCentro $ EsCuadro $ EspecialidadDeGraduacion $ CentroDeGraduacion))

objectclass ( 1.3.6.1.4.1.53027.1.4.4 NAME 'Externo'
	DESC 'Trabajador Externo de la Universidad de la Habana'
	SUP Todos STRUCTURAL
	MUST (uid) 
	MAY ( Id $ TiempoDeActividad ))

